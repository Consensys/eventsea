{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["NEXT_PUBLIC_EVENTS_FACTORY_CONTRACT_ADDRESS", "INFURA_IPFS_ENDPOINT", "INFURA_IPFS_KEY", "INFURA_IPFS_SECRET" ],
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "network": {
      "cache": false,
      "persistent": true
    }
  }
}